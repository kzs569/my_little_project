function createPageHTML(_nPageCount, _nCurrIndex, _sPageName, _sPageExt){

	if(_nPageCount == null || _nPageCount<=1){
		return;
	}

var nCurrIndex = _nCurrIndex || 0;
var currentPage =nCurrIndex+1;//所在页从1开始
var countPage =_nPageCount;//共多少页
var nextPage=currentPage+1;
var prePage=currentPage-1;

var html="";


if(countPage>1&&currentPage!=1)
	html=html+"<a href=\""+_sPageName+"."+_sPageExt+"\"  >首页</a><a href=\""+_sPageName+"."+_sPageExt+"\"  >上一页</a>";
else 	html=html+"<a  >首页&nbsp;</a><a >上一页</a>";
	


if(currentPage<=5){
	for(var i=1;i<=countPage ;i++){

		if(i==currentPage){
			html=html+"<a class=\"hover\">"+i+"</a>";
		}else if(i==1){

html=html+"<a href=\""+_sPageName+"."+_sPageExt+"\"  >"+i+"</a>";

                   }else{
			html=html+"<a href=\""+_sPageName+"_" + (i-1) + "."+_sPageExt+"\" >"+i+"</a>";
		}

	}

}else if((countPage-currentPage)<=5&&(countPage-9)>0){

	for(var i=(countPage-9);i<=countPage ;i++){

		if(i==currentPage){
			html=html+"<a  class=\"hover\">"+i+"</a>";
		}else if(i==1){

html=html+"<a href=\""+_sPageName+"."+_sPageExt+"\"  >"+i+"</a>";

                   }else{
			html=html+"<a href=\""+_sPageName+"_" + (i-1) + "."+_sPageExt+"\" >"+i+"</a>";
		}

	}

}else if((countPage-currentPage)<=5&&(countPage-9)<=0){

	for(var i=1;i<=countPage ;i++){

		if(i==currentPage){
			html=html+"<a class=\"hover\">"+i+"</a>";
		}else if(i==1){

html=html+"<a href=\""+_sPageName+"."+_sPageExt+"\"  >"+i+"</a>";

                   }else{
			html=html+"<a href=\""+_sPageName+"_" + (i-1) + "."+_sPageExt+"\" >"+i+"</a>";
		}

	}

}else{

	for(var i=(currentPage-5);i<=(currentPage+5);i++){

		if(i==currentPage){
			html=html+"<a class=\"hover\">"+i+"</a>";
		}else if(i==1){

html=html+"<a href=\""+_sPageName+"."+_sPageExt+"\"  >"+i+"</a>";

                   }else{
			html=html+"<a href=\""+_sPageName+"_" + (i-1) + "."+_sPageExt+"\" >"+i+"</a>";
		}

	}

}



//设置下一页代码 
if(countPage>1&&currentPage!=countPage)
	html=html+"<a href=\""+_sPageName+"_" + currentPage + "."+_sPageExt+"\"   >下一页</a><a href=\""+_sPageName+"_" + (countPage-1)+ "."+_sPageExt+"\"   >尾页</a>";
else
	html=html+"<a   >下一页</a><a    >尾页</a>";

 

html=html+"<a  >共"+countPage+"页</a>"
	
 

 
document.write(html);

}
